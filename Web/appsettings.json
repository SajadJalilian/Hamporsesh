{
  "ConnectionStrings": {
    "MainConnection": "Server=.;Database=Hamporsesh.Development;Trusted_Connection=True;MultipleActiveResultSets=true",
    "DbLoggerConnection": "Server=.;Database=Hamporsesh.Development;Trusted_Connection=True;MultipleActiveResultSets=true"

  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Options": {
      "Password": {
        "RequireLowercase": false,
        "RequireUppercase": false,
        "RequireNonAlphanumeric": false,
        "RequiredLength": 4,
        "RequireDigit": false
      },
      "Lockout": {
        "AllowedForNewUsers": false,
        "DefaultLockoutTimeSpan": 30,
        "MaxFailedAccessAttempts": 10
      },
      "User": {
        "RequireUniqueEmail": false
      }
    }
  }
}
